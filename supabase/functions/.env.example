# Development Mode Configuration
# Set DEV_MODE=true to use manual user data instead of SSO decryption
DEV_MODE=true

# Manual User Data for Development (only used when DEV_MODE=true)
# Replace these with your actual GoHighLevel user details
DEV_USER_ID=user_12345
DEV_USER_EMAIL=test@example.com
DEV_USER_NAME=Test User
DEV_COMPANY_ID=comp_67890
DEV_LOCATION_ID=loc_54321
DEV_USER_ROLE=admin
DEV_USER_TYPE=location
DEV_ACTIVE_LOCATION=loc_54321

# Production Configuration
# Your GoHighLevel app's shared secret for SSO decryption
GHL_APP_SHARED_SECRET=your-shared-secret-here