# Development Mode Configuration
# Set DEV_MODE=true to use manual user data instead of SSO decryption
DEV_MODE=true

# CRITICAL: Use FAKE/TEST IDs for development - NEVER use real production IDs
# These should be completely different from your actual GHL account IDs
DEV_USER_ID=dev_user_12345
DEV_USER_EMAIL=dev@example.com
DEV_USER_NAME=Dev User
DEV_LOCATION_ID=dev_location_12345
DEV_COMPANY_ID=dev_company_12345
DEV_ACTIVE_LOCATION=dev_location_12345
DEV_USER_ROLE=admin
DEV_USER_TYPE=agency

# Production Configuration
# Your GoHighLevel app's shared secret for SSO decryption
GHL_APP_SHARED_SECRET=your-actual-shared-secret-here